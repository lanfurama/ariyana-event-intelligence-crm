{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/v1/(.*)",
      "destination": "/api/v1/[...path]"
    }
  ],
  "functions": {
    "api/v1/[...path].ts": {
      "maxDuration": 30,
      "includeFiles": "api/src/**"
    }
  }
}
